{
  "name": "scourjs",
  "description": "Traverse objects and arrays",
  "version": "0.8.1",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browser": "dist/scour.min.js",
  "bugs": {
    "url": "https://github.com/rstacruz/scour/issues"
  },
  "dependencies": {
    "object-assign": "4.0.1",
    "sift": "https://github.com/rstacruz/sift.js/tarball/feature/sift-by-object",
    "to-function": "2.0.6"
  },
  "devDependencies": {
    "babel": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "benchmark": "1.0.0",
    "browserify": "12.0.1",
    "docpress": "0.6.10",
    "expect": "1.13.4",
    "git-update-ghpages": "1.3.0",
    "istanbul": "0.4.1",
    "mdx": "0.3.1",
    "mocha": "2.3.4",
    "mocha-standard": "1.0.0",
    "standard": "5.4.1",
    "tap-difflet": "0.4.0",
    "tape": "4.4.0",
    "uglifyjs": "2.4.10",
    "watchify": "3.6.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/rstacruz/scour#readme",
  "keywords": [
    "array",
    "immutable",
    "object"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstacruz/scour.git"
  },
  "scripts": {
    "build": "browserify -t babelify --bare -s scour scour.js > dist/scour.js; uglifyjs dist/scour.js -cm > dist/scour.min.js",
    "coverage": "istanbul cover _mocha -- -R spec",
    "prepublish": "make -B update && npm run build",
    "test": "mocha",
    "test:tape": "node test/index.js | tap-difflet",
    "watch": "watchify -v -t babelify --bare -s scour scour.js -o dist/scour.js"
  }
}
